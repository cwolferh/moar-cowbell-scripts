    rbd_max_clone_depth: '5'
    rbd_pool: volumes
    secret_uuid: '77123ba2-ed14-4f28-a645-62699c647c53'
    rbd_user: volumes
    rpc_backend: cinder.openstack.common.rpc.impl_kombu
    san_ip:
